#include <iostream>
#include <cmath> 
using namespace std;

int absolute(int num) {
  return abs(num);
}
float absolute(float num) {
  return abs(num); 
}

int main() {
  int num1 = -5;
  float num2 = -3.14;
cout << "Absolute value of " << num1 << " is " << absolute(num1) << endl;
cout << "Absolute value of " << num2 << " is " << absolute(num2) << endl;

return 0;
           }
