#include <iostream>
using namespace std;
int max(int n1,int n2) {
  if(n1>n2) {
    return n1;
  }
  return n2;
}
float max(float n1, float n2) {
  if(n1>n2) {
    return n1;
  }
  return n2;
}
char max(char ch1,char ch2) {
  if(ch1>ch2) {
    return ch1;
  }
  return ch2;
}
int main () {
  int n5=5,n6=6;
  float n3=2.5,n4=6.3;
  char c1='a',c2='A';
  cout<<"Maximum number is: "<<max(n5,n6)<<endl;
  cout<<"Maximun number is: "<<max(n3,n4)<<endl;
  cout<<"maximum character is: "<<max(c1,c2)<<endl;
  return 0;
}
